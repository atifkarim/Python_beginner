class calculator:
    def addition(x,y):
        added = x + y
        print(added)

    def subtraction(x, y):
        sub = x - y
        print(sub)

    def multiplication(x, y):
        mult = x * y
        print(mult)

    def division(x, y):
        div = x / y
        print(div)
calculator.addition(x=int(input()),y=int(input()))


x = input('What is your name?: ')
print('Hello',x)

y=float(input())
print ("the num is: ",y)